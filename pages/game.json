{
  "dataSets": {
    "layout": {
      "viewlet": "pageContainer",
      "titleBar": {
        "viewlet": "gameTitleBar",
        "width": "stretchToParent",
        "backgroundColor": "#fff",
        "localizedTitle": "GLOBAL_APP_NAME",
        "showDivider": true
      },
      "bottomBar": {
        "viewlet": "simpleBottomBar",
        "width": "stretchToParent",
        "backgroundColor": "#ddd"
      },
      "backgroundItem": {
        "viewlet": "view",
        "width": "stretchToParent",
        "height": "stretchToParent",
        "backgroundColor": "#fff"
      },
      "contentItems": [
        {
          "viewlet": "frameContainer",
          "width": "stretchToParent",
          "height": "stretchToParent",
          "padding": [ "$pagePadding", "$pagePadding", "$pagePadding", "$pagePadding" ],
          "backgroundColor": "#ddd",
          "items": [
            {
              "viewlet": "levelCanvas",
              "canvasWidth": 12,
              "canvasHeight": 16,
              "gravity": "center",
              "backgroundColor": "#fff"
            }
          ]
        }
      ]
    }
  }
}
